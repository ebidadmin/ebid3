<% unless item.part_no.blank? %>
	<%= item.part_no %>
	<%= link_to "change", cart_show_fields_path(:item => item), :id => 'change-partnum-button', :class => 'button', :remote => true %>
<% else %>
	<%= link_to "click to specify details", cart_show_fields_path(:item => item), :class => 'button', :remote => true %>
<% end %>