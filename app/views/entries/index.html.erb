<% title "Entries" %>

<table>
  <tr>
    <th>User</th>
    <th>Ref No</th>
    <th>Year Model</th>
    <th>Car Brand</th>
    <th>Car Model</th>
    <th>Car Variant</th>
    <th>Plate No</th>
    <th>Serial No</th>
    <th>Motor No</th>
    <th>Date Of Loss</th>
    <th>City</th>
    <th>Term</th>
    <th>Bid Until</th>
    <th>Buyer Status</th>
    <th>Photos Count</th>
    <th>Additional Flag</th>
    <th>Expired</th>
    <th>Chargeable Expiry</th>
  </tr>
  <% for entry in @entries %>
    <tr>
      <td><%= entry.user_id %></td>
      <td><%= entry.ref_no %></td>
      <td><%= entry.year_model %></td>
      <td><%= entry.car_brand_id %></td>
      <td><%= entry.car_model_id %></td>
      <td><%= entry.car_variant_id %></td>
      <td><%= entry.plate_no %></td>
      <td><%= entry.serial_no %></td>
      <td><%= entry.motor_no %></td>
      <td><%= entry.date_of_loss %></td>
      <td><%= entry.city_id %></td>
      <td><%= entry.term_id %></td>
      <td><%= entry.bid_until %></td>
      <td><%= entry.buyer_status %></td>
      <td><%= entry.photos_count %></td>
      <td><%= entry.additional_flag %></td>
      <td><%= entry.expired %></td>
      <td><%= entry.chargeable_expiry %></td>
      <td><%= link_to "Show", entry %></td>
      <td><%= link_to "Edit", edit_entry_path(entry) %></td>
      <td><%= link_to "Destroy", entry, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Entry", new_entry_path %></p>
