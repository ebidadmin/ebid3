<%= form_for @line_item do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :entry_id %><br />
    <%= f.text_field :entry_id %>
  </p>
  <p>
    <%= f.label :car_part_id %><br />
    <%= f.text_field :car_part_id %>
  </p>
  <p>
    <%= f.label :part_no %><br />
    <%= f.text_field :part_no %>
  </p>
  <p>
    <%= f.label :quantity %><br />
    <%= f.text_field :quantity %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.label :bids_count %><br />
    <%= f.text_field :bids_count %>
  </p>
  <p><%= f.submit %></p>
<% end %>
