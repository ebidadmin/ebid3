<% title "Car Variants" %>

<table>
  <tr>
    <th>Car Brand</th>
    <th>Car Model</th>
    <th>Name</th>
    <th>Start Year</th>
    <th>End Year</th>
  </tr>
  <% for car_variant in @car_variants %>
    <tr>
      <td><%= car_variant.car_brand_id %></td>
      <td><%= car_variant.car_model_id %></td>
      <td><%= car_variant.name %></td>
      <td><%= car_variant.start_year %></td>
      <td><%= car_variant.end_year %></td>
      <td><%= link_to "Show", car_variant %></td>
      <td><%= link_to "Edit", edit_car_variant_path(car_variant) %></td>
      <td><%= link_to "Destroy", car_variant, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Car Variant", new_car_variant_path %></p>
